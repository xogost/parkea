.navigation li a{color:#fff; text-decoration:none;}
.navigation li:hover {background:#37616E; text-decoration:none; -moz-box-shadow:inset 0px 2px 10px #000000; -webkit-box-shadow:inset 0px 2px 10px #000000; box-shadow:inset 0px 2px 10px #000000;}
