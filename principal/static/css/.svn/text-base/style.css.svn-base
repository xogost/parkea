#recient_activity li img {width:32px; transition: all 500ms ease; -moz-transition: all 500ms ease; /* Firefox 4 */ -webkit-transition: all 500ms ease; /* Safari and Chrome */ -o-transition: all 500ms ease; /* Opera */}
